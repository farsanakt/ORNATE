<style>
    .text-animation {
    text-align: center;
}

.text-animation h2,
.text-animation p {
    animation: fadeInOut 4s infinite alternate;
    opacity: 0;
}

.text-animation h2 {
    font-size: 36px;
    color: #333; /* Change color to your preference */
}

.text-animation p {
    font-size: 18px;
    color: #666; /* Change color to your preference */
}

@keyframes fadeInOut {
    0% {
        opacity: 0;
    }
    50% {
        opacity: 1;
    }
    100% {
        opacity: 0;
    }
}

</style>
<style>
    @keyframes rotateText {
        from {
            transform: rotate(0deg);
        }
        to {
